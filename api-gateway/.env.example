# .env.example

# URL de conexão com o banco PostgreSQL (exemplo)
DATABASE_URL=postgresql://usuario:senha@host:porta/banco?sslmode=require

# Porta onde a aplicação irá rodar
PORT=3000

# Segredo JWT (token de autenticação)
JWT_SECRET=uma_chave_jwt_secreta_qualquer

# URLs dos serviços usados pela aplicação
AUTH_URL=http://localhost:3001
ENTRADAS_URL=http://localhost:3002
CREDORES_URL=http://localhost:3003
SAIDAS_URL=http://localhost:3004
FINANCES_URL=http://localhost:3005

