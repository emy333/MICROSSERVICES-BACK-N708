# .env.example

# URL de conexão com o banco PostgreSQL
DATABASE_URL=postgresql://usuario:senha@host:porta/nome_do_banco?sslmode=require

# Porta onde a aplicação irá rodar
PORT=3003

# Segredo para geração de JWT (token de autenticação)
JWT_SECRET=uma_chave_jwt_secreta_aqui

# URLs dos serviços da aplicação
USUARIO_SERVICE_URL=http://localhost:3000/usuarios
SAIDAS_SERVICE_URL=http://localhost:3000/saidas
